{
  "name": "n8n-nodes-google-forms",
  "version": "1.0.0",
  "description": "Automate Google Forms(TM) tasks with n8n. Create, manage, and analyze forms effortlessly",
  "keywords": ["n8n-community-node-package", "n8n", "google", "forms"],
  "license": "MIT",
  "homepage": "https://axiomlab.dev",
  "author": {
    "name": "Axiomlab.dev",
    "email": "mustakim@axiomlab.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MustakimMasum/n8n-nodes-google-forms.git"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/gForms/gForms.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
